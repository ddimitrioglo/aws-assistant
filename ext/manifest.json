{
  "key": "",
  "name": "KubeAuth",
  "version": "0.1.2",
  "manifest_version": 2,
  "description": "An easy way to login into Kubernetes dashboard",
  "icons": {
    "128": "img/icon-128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": true
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "nativeMessaging",
    "webRequest",
    "webRequestBlocking"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.jsdelivr.net; object-src 'self'"
}
