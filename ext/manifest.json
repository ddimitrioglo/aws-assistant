{
  "name": "KubeAuth",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Easy way to login into kubernetes dashboard",
  "icons": {
    "128": "img/icon-128.png"
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["js/jquery-3.5.1.min.js", "js/content.js" ]
  }],
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  }
}
